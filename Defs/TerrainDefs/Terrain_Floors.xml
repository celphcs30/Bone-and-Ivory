<Defs>

  <TerrainDef ParentName="FloorBase">
    <defName>IvoryTile</defName>
    <label>ivory tile</label>
    <renderPrecedence>242</renderPrecedence>
    <description>On the XI Centaury y'all be on jail for this.</description>
    <texturePath>Terrain/Surfaces/GenericFloorTile</texturePath>
    <color>(0.95, 0.9, 0.8)</color>
    <isPaintable>true</isPaintable>
    <statBases>
      <Beauty>12</Beauty>
      <WorkToBuild>750</WorkToBuild>
      <Cleanliness>0.2</Cleanliness>
    </statBases>
    <costList>
      <Ivory>8</Ivory>
    </costList>
	<constructEffect>ConstructMetal</constructEffect>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
    <tags>
      <li>Floor</li>
    </tags>
    <constructionSkillPrerequisite>3</constructionSkillPrerequisite>
  </TerrainDef>
  
